<div class="form-group row">
  <label class="col-form-label col-sm-2" for="cusid">ID</label>
  <div class="col-sm-8">
    <input readonly class="form-control" id="cusid" value="<%= model.cusid %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="cusfname">First Name</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="cusfname" value="<%= model.cusfname %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="cuslname">Last Name</label>
  <div class="col-sm-10">
    <textarea readonly class="form-control" cols="20" id="cuslname" maxlength="32000" rows="7"><%= model.cuslname %></textarea>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="cusstate">State</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="cusstate" value="<%= model.cusstate %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="cussalesytd">Sales YTD</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="cussalesytd" value="<%= model.cussalesytd %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="cussalesprev">Previous Years Sales</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="cussalesprev" value="<%= model.cussalesprev %>" />
  </div>
</div>